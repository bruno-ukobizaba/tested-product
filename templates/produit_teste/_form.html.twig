{{ form_start(formProduitTeste)}}
{{ form_errors(formProduitTeste) }}

<div class="form-group">
    {{ form_row(formProduitTeste.IUPAC) }}
    {{ form_row(formProduitTeste.numeroCAS) }}
    {{ form_row(formProduitTeste.isMelange) }}
    {{ form_row(formProduitTeste.isTested) }}
    {{ form_row(formProduitTeste.isActive) }}
    {{ form_row(formProduitTeste.dangerosite) }}
    {{ form_row(formProduitTeste.solution) }}
</div>
<div class="form-group">
<ul class="tags">
    {% for caracteristiqueForm in formProduitTeste.caracteristiques %}
    <li>
        {{ form_errors(caracteristiqueForm) }}
        {{ form_row(caracteristiqueForm.nomCommun) }}
        {{ form_row(caracteristiqueForm.description) }}
    </li>
{% endfor %}
</ul>
</div>     

<button type="submit" class="btn btn-success">Enregistrer</button>
{{ form_end(formProduitTeste)}}
